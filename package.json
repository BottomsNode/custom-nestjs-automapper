{
    "name": "custom-automapper",
    "version": "1.0.8",
    "description": "A powerful, type-safe object mapping library for TypeScript and NestJS",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint \"src/**/*.ts\"",
        "lint:fix": "eslint --fix \"src/**/*.ts\"",
        "format": "prettier --write \"src/**/*.ts\"",
        "release": "npm version patch -m \"Release %s\" && git push origin main --tags && npm publish --access public"
    },
    "keywords": [
        "automapper",
        "mapper",
        "object-mapper",
        "dto",
        "nestjs",
        "typescript",
        "mapping",
        "transformation"
    ],
    "author": "Nishit Shivdasani <mysocialmailexpress@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/BottomsNode/custom-automapper.git"
    },
    "homepage": "https://github.com/BottomsNode/custom-automapper#readme",
    "bugs": {
        "url": "https://github.com/BottomsNode/custom-automapper/issues"
    },
    "peerDependencies": {
        "@nestjs/common": "^10.0.0 || ^11.0.0",
        "@nestjs/core": "^10.0.0 || ^11.0.0",
        "reflect-metadata": "^0.1.13 || ^0.2.0",
        "rxjs": "^7.0.0"
    },
    "peerDependenciesMeta": {
        "@nestjs/common": {
            "optional": true
        },
        "@nestjs/core": {
            "optional": true
        },
        "rxjs": {
            "optional": true
        }
    },
    "dependencies": {
        "reflect-metadata": "^0.2.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.38.0",
        "@nestjs/common": "^11.0.0",
        "@nestjs/core": "^11.0.0",
        "@nestjs/testing": "^11.0.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.0.0",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^9.38.0",
        "globals": "^16.4.0",
        "jest": "^30.2.0",
        "prettier": "^3.0.0",
        "rxjs": "^7.8.0",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.5",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.46.2"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "engines": {
        "node": ">=16.0.0"
    }
}
